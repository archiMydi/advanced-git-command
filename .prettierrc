{
    "tabWidth": 4,
    "useTabs": true,
    "singleQuote": false,
    "trailingComma": "all",
    "printWidth": 100,
    "bracketSameLine": false,
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [
        { "files": "*.html", "options": { "useTabs": true } },
        { "files": "./package.json", "options": { "useTabs": true } }
    ]
}
